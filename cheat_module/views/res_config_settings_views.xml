<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_config_settings_pos_order_notes" model="ir.ui.view">
        <field name="name">pos.config.form.view</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="base_setup.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@id='companies']" position="after">
                <block title="Cheats" name="contacts_setting_container">
                    <setting id="cheat_setting" string="Cheat Setting" help="This is the cheat setting.">
                        <field name="cheat_setting"/>
                    </setting>
                </block>
            </xpath>
        </field>
    </record>
</odoo>